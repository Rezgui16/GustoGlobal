@{
    Layout = "_Layout";
    ViewData["Title"] = "_LayoutForm";
}


<div class="card">
    <div class="card-body">
        <h5 class="card-title">@RenderSection("SectionTitle", true)</h5>
        @RenderBody()
    </div>
</div>

@section SectionScripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    @RenderSection("SectionScripts", false);
}